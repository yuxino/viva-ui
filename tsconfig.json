{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "module": "ES2015",
    "moduleResolution": "node",
    // "noImplicitAny": true,
    "preserveConstEnums": true,
    "target": "es5",
    "declaration": true,
    "sourceMap": true
  },
  "exclude": ["node_modules"]
}
